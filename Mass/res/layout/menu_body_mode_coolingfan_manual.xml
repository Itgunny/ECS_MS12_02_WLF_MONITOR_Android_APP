<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Relativelayout_menu_body_mode_coolingfan_manual_low"
        android:layout_alignParentTop="true" >

		        
	        <ImageView
	            android:id="@+id/imageView_menu_body_mode_coolingfan_manual"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerVertical="true"
	            android:layout_marginLeft="100dp"
	            android:src="@drawable/menu_mode_fan_manual_icon_off" />		        
		    
		    <TextView
		        android:id="@+id/textView_menu_body_mode_coolingfan_manual"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_alignParentRight="true"
		        android:layout_centerVertical="true"
		        android:layout_marginRight="60dp"
		        android:background="@drawable/_selector_menu_body_mode_coolingfan_manual_btn1"
		        android:gravity="center"
		        android:text="@string/Excute"
		        android:textAppearance="?android:attr/textAppearanceLarge"
		        android:textColor="@drawable/_selector_radio_txt_white_black"
		        android:textSize="40sp" />

		    <TextView
		        android:id="@+id/textView_menu_body_mode_coolingfan_manual_description"
		        android:layout_width="wrap_content"
		        android:layout_height="wrap_content"
		        android:layout_below="@+id/imageView_menu_body_mode_coolingfan_manual"
		        android:text="@string/Push_and_hold_to_reverse_rotation"
		        android:layout_marginLeft="40dp"
		        android:layout_marginTop="10dp"
		        android:textAppearance="?android:attr/textAppearanceLarge"
		        android:textColor="@color/menu_body_monitoring_version_data_gray"
		        android:textSize="20sp" />
		    
    </RelativeLayout>

    
    
    <RelativeLayout
        android:id="@+id/Relativelayout_menu_body_mode_coolingfan_manual_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >
       
        <ImageView
            android:id="@+id/imageView_menu_body_mode_coolingfan_manual_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_mode_coolingfan_manual_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_mode_coolingfan_manual_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_mode_coolingfan_manual_low_line"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:src="@drawable/main_key_low_line" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_mode_coolingfan_manual_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	                android:layout_alignTop="@+id/ImageButton_menu_body_mode_coolingfan_manual_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_mode_coolingfan_manual_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />

	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>
    

</RelativeLayout>
