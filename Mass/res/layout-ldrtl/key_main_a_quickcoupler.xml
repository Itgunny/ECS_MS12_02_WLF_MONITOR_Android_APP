<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout_key_main_a_quickcoupler"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_a_bg_right" 
    android:splitMotionEvents="false">

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_a_quickcoupler_available"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:splitMotionEvents="false">

        <taeha.wheelloader.fseries_monitor.main.TextFitTextView
            android:id="@+id/textView_key_main_a_quickcoupler_lock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="251dp"
            android:layout_marginTop="75dp"
            android:background="@drawable/_selector_key_quickcoupler_btn1"
            android:gravity="center"
            android:text="@string/Locking_Attachment"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <taeha.wheelloader.fseries_monitor.main.TextFitTextView
            android:id="@+id/textView_key_main_a_quickcoupler_unlock"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/textView_key_main_a_quickcoupler_lock"
            android:layout_below="@+id/textView_key_main_a_quickcoupler_lock"
            android:layout_marginTop="30dp"
            android:background="@drawable/_selector_key_quickcoupler_btn1"
            android:gravity="center"
            android:text="@string/Unlocking_Attachment"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <RelativeLayout
            android:id="@+id/RelativeLayout_key_main_a_quickcoupler_low"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="280dp" 
            android:splitMotionEvents="false">

            <ImageView
                android:id="@+id/imageView_key_main_a_quickcoupler_low_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/main_a_key_low_bg" />

            <ImageView
                android:id="@+id/imageView_key_main_a_quickcoupler_low_line"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView_key_main_a_quickcoupler_low_bg"
                android:layout_toLeftOf="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                android:src="@drawable/main_key_low_line" />

            <ImageButton
                android:id="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/imageView_key_main_a_quickcoupler_low_bg"
                android:layout_alignRight="@+id/imageView_key_main_a_quickcoupler_low_bg"
                android:background="@drawable/_selector_key_centerright_btn2" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                android:layout_alignLeft="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                android:layout_alignRight="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                android:layout_alignTop="@+id/ImageButton_key_main_a_quickcoupler_low_ok" 
                android:splitMotionEvents="false">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true" 
                    android:splitMotionEvents="false">

                    <ImageView
                        android:id="@+id/imageView_key_main_a_quickcoupler_low_ok_icon"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBottom="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                        android:layout_alignLeft="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                        android:layout_alignTop="@+id/ImageButton_key_main_a_quickcoupler_low_ok"
                        android:src="@drawable/menu_low_icon_ok" />

                    <taeha.wheelloader.fseries_monitor.main.TextFitTextView
                        android:id="@+id/textView_key_main_a_quickcoupler_low_ok"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:layout_marginLeft="10dp"
                        android:gravity="center"
                        android:text="@string/OK"
                        android:textAppearance="?android:attr/textAppearanceLarge"
                        android:textSize="25sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_a_quickcoupler_notavailable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="205dp"
        android:visibility="invisible" 
        android:splitMotionEvents="false">

        <taeha.wheelloader.fseries_monitor.main.TextFitTextView
            android:id="@+id/textView_key_main_a_quickcoupler_notavailable_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/Quick_Coupler_is_NOT_equipped"
            android:textColor="@drawable/_selector_radio_txt_white_amber"
            android:textSize="25sp" />
    </RelativeLayout>

</RelativeLayout>