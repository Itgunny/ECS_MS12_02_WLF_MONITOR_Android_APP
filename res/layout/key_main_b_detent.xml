<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout_key_main_b_detent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_key_bg" >

    <ImageView
        android:id="@+id/imageView_key_main_b_detent_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="168dp"
        android:src="@drawable/main_key_line" />

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_detent_boom"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView_key_main_b_detent_line"
        android:layout_alignRight="@+id/imageView_key_main_b_detent_line"
        android:layout_marginTop="65dp" >

        <ImageView
            android:id="@+id/imageView_key_main_b_detent_boom_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_detent_boom_title"
            android:layout_alignTop="@+id/textView_key_main_b_detent_boom_title"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_key_main_b_detent_boom_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text="@string/Boom"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <RadioGroup
            android:id="@+id/radioGroup_key_main_b_detent_boom"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_detent_boom_saveposition"
            android:layout_alignLeft="@+id/textView_key_main_b_detent_boom_title"
            android:orientation="horizontal" >
	
	       <RadioButton
	           android:id="@+id/radioButton_key_main_b_detent_boom_off"
	           android:layout_width="150dp"
	           android:layout_height="wrap_content"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/Off"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp"
	           android:textStyle="bold" />
	
	       <RadioButton
	           android:id="@+id/radioButton_key_main_b_detent_boom_on"
	           android:layout_width="150dp"
	           android:layout_height="wrap_content"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/On"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp"
	           android:textStyle="bold" />
	       
	       
	
	   </RadioGroup>

        <TextView
            android:id="@+id/textView_key_main_b_detent_boom_saveposition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="320dp"
            android:background="@drawable/_selector_key_detent_btn1"
            android:gravity="center"
            android:paddingTop="15dp"
            android:text="@string/Save_Position"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_detent_bucket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView_key_main_b_detent_line"
        android:layout_alignRight="@+id/imageView_key_main_b_detent_line"
        android:layout_below="@+id/imageView_key_main_b_detent_line"
        android:layout_marginTop="10dp" >

        <ImageView
            android:id="@+id/imageView_key_main_b_detent_bucket_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_detent_bucket_title"
            android:layout_alignTop="@+id/textView_key_main_b_detent_bucket_title"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_key_main_b_detent_bucket_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text="@string/Bucket"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <RadioGroup
            android:id="@+id/radioGroup_key_main_b_detent_bucket"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_detent_bucket_saveposition"
            android:layout_alignLeft="@+id/textView_key_main_b_detent_bucket_title"
            android:orientation="horizontal" >
	
	       <RadioButton
	           android:id="@+id/radioButton_key_main_b_detent_bucket_off"
	           android:layout_width="150dp"
	           android:layout_height="wrap_content"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/Off"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp"
	           android:textStyle="bold" />
	
	       <RadioButton
	           android:id="@+id/radioButton_key_main_b_detent_bucket_on"
	           android:layout_width="150dp"
	           android:layout_height="wrap_content"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/On"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp"
	           android:textStyle="bold" />
	       
	       
	
	   </RadioGroup>

        <TextView
            android:id="@+id/textView_key_main_b_detent_bucket_saveposition"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="320dp"
            android:background="@drawable/_selector_key_detent_btn1"
            android:gravity="center"
            android:paddingTop="15dp"
            android:text="@string/Save_Position"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_detent_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" 
        android:layout_marginLeft="52dp"
	    android:layout_marginTop="303dp">
        
        <ImageView
	        android:id="@+id/imageView_key_main_b_detent_low_bg"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        
	        android:src="@drawable/main_key_low_bg" />
	
	    <ImageView
	        android:id="@+id/imageView_key_main_b_detent_low_line"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_detent_low_bg"
	        android:layout_alignRight="@+id/ImageButton_key_main_b_detent_low_ok"
	        android:layout_marginRight="190dp"
	        android:src="@drawable/main_key_low_line" />
	
	    
	
	    <ImageButton
	        android:id="@+id/ImageButton_key_main_b_detent_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_detent_low_bg"
	        android:layout_alignRight="@+id/imageView_key_main_b_detent_low_bg"
	        android:background="@drawable/_selector_key_centerright_btn1" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_key_main_b_detent_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_key_main_b_detent_low_ok"
	        android:layout_alignRight="@+id/ImageButton_key_main_b_detent_low_ok"
	        android:layout_alignTop="@+id/ImageButton_key_main_b_detent_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_key_main_b_detent_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_key_main_b_detent_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_key_main_b_detent_low_ok"
	                android:layout_alignTop="@+id/ImageButton_key_main_b_detent_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_key_main_b_detent_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>

    

</RelativeLayout>