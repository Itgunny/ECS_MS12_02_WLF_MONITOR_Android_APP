<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_operationhistory_currentfuelrate"
        android:layout_width="742dp"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:background="@drawable/menu_information_monitoring_block" >

        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_currentfuelrate_title"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_marginLeft="40dp"
            android:gravity="center_vertical"
            android:text="@string/Current_Fuel_Rate"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_currentfuelrate_data"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="270dp"
            android:gravity="right|center_vertical"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_currentfuelrate_unit"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="230dp"
            android:gravity="center_vertical"
            android:text="@string/l_h"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/menu_body_mode_tab_gray" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_operationhistory_latesthour"
        android:layout_width="742dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RelativeLayout_menu_body_monitoring_operationhistory_currentfuelrate"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="7dp"
        android:background="@drawable/menu_information_monitoring_block" >

        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latesthour_title"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_marginLeft="40dp"
            android:gravity="center_vertical"
            android:text="@string/Latest_Hourmeter"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latesthour_data"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="270dp"
            android:gravity="right|center_vertical"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latesthour_unit"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="230dp"
            android:gravity="center_vertical"
            android:text="@string/Hr"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/menu_body_mode_tab_gray" />
        
        <TextView
            android:id="@+id/TextView_menu_body_monitoring_operationhistory_latesthour_init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/_selector_menu_body_monitoring_machinemonitoring_btn1"
            android:gravity="center"
            android:text="@string/Initialization"
            android:textColor="@drawable/_selector_menu_body_multimedia_txt"
            android:textSize="20sp" />

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_operationhistory_totalodo"
        android:layout_width="742dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RelativeLayout_menu_body_monitoring_operationhistory_latesthour"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="7dp"
        android:background="@drawable/menu_information_monitoring_block" >

        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_totalodo_title"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_marginLeft="40dp"
            android:gravity="center_vertical"
            android:text="@string/Total_Odometer"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_totalodo_data"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="270dp"
            android:gravity="right|center_vertical"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_totalodo_unit"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="230dp"
            android:gravity="center_vertical"
            android:text="@string/km"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/menu_body_mode_tab_gray" />

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_operationhistory_latestodo"
        android:layout_width="742dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/RelativeLayout_menu_body_monitoring_operationhistory_totalodo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="7dp"
        android:background="@drawable/menu_information_monitoring_block" >

        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latestodo_title"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_marginLeft="40dp"
            android:gravity="center_vertical"
            android:text="@string/Latest_Odometer"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latestodo_data"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="270dp"
            android:gravity="right|center_vertical"
            android:text="0"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_monitoring_operationhistory_latestodo_unit"
            android:layout_width="wrap_content"
            android:layout_height="76dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="230dp"
            android:gravity="center_vertical"
            android:text="@string/km"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/menu_body_mode_tab_gray" />
        
        <TextView
            android:id="@+id/TextView_menu_body_monitoring_operationhistory_latestodo_init"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:background="@drawable/_selector_menu_body_monitoring_machinemonitoring_btn1"
            android:gravity="center"
            android:text="@string/Initialization"
            android:textColor="@drawable/_selector_menu_body_multimedia_txt"
            android:textSize="20sp" />

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/Relativelayout_menu_body_monitoring_operationhistory_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >
       
        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_operationhistory_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_monitoring_operationhistory_low_line"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:src="@drawable/main_key_low_line" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_monitoring_operationhistory_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_monitoring_operationhistory_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_monitoring_operationhistory_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	                android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_operationhistory_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_monitoring_operationhistory_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />

	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>
