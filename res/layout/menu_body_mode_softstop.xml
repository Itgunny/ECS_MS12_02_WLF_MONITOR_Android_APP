<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <ImageView
        android:id="@+id/imageView_menu_body_mode_softstop_line1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="86dp"
        android:src="@drawable/line_menu_step1" />

	<ImageView
	    android:id="@+id/imageView_menu_body_mode_softstop_line2"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_below="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_marginTop="86dp"
	    android:src="@drawable/line_menu_step1" />

	<ImageView
	    android:id="@+id/imageView_menu_body_mode_softstop_line3"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_below="@+id/imageView_menu_body_mode_softstop_line2"
	    android:layout_marginTop="86dp"
	    android:src="@drawable/line_menu_step1" />
	
	<ImageView
	    android:id="@+id/imageView_menu_body_mode_softstop_line4"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_below="@+id/imageView_menu_body_mode_softstop_line3"
	    android:layout_marginTop="86dp"
	    android:src="@drawable/line_menu_step1" />

	<RelativeLayout
	    android:id="@+id/RelativeLayout_menu_body_mode_softstop_boomup"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignParentTop="true"
	    android:layout_alignRight="@+id/imageView_menu_body_mode_softstop_line1" >
	    
	    <TextView
	        android:id="@+id/textView_menu_body_mode_softstop_boomup"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentTop="true"
	        android:gravity="center_vertical"
	        android:text="@string/Boom_Up"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_boomup_on"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="296dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:gravity="left|center_vertical"
		    android:text="@string/On"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_boomup_off"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="503dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:text="@string/Off"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

	</RelativeLayout>
	
	<RelativeLayout
	    android:id="@+id/RelativeLayout_menu_body_mode_softstop_boomdown"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_line2"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignRight="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignTop="@+id/imageView_menu_body_mode_softstop_line1" >
	    
	    <TextView
	        android:id="@+id/textView_menu_body_mode_softstop_boomdown"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentTop="true"
	        android:gravity="center_vertical"
	        android:text="@string/Boom_Down"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_boomdown_on"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="296dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:gravity="left|center_vertical"
		    android:text="@string/On"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_boomdown_off"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="503dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:text="@string/Off"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

	</RelativeLayout>
	
	<RelativeLayout
	    android:id="@+id/RelativeLayout_menu_body_mode_softstop_bucketin"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_line3"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignRight="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignTop="@+id/imageView_menu_body_mode_softstop_line2" >
	    
	    <TextView
	        android:id="@+id/textView_menu_body_mode_softstop_bucketin"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentTop="true"
	        android:gravity="center_vertical"
	        android:text="@string/Bucket_In"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_bucketin_on"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="296dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:gravity="left|center_vertical"
		    android:text="@string/On"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_bucketin_off"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="503dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:text="@string/Off"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

	</RelativeLayout>
	
	<RelativeLayout
	    android:id="@+id/RelativeLayout_menu_body_mode_softstop_bucketout"
	    android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_line4"
	    android:layout_alignLeft="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignRight="@+id/imageView_menu_body_mode_softstop_line1"
	    android:layout_alignTop="@+id/imageView_menu_body_mode_softstop_line3" >
	    
	    <TextView
	        android:id="@+id/textView_menu_body_mode_softstop_bucketout"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentBottom="true"
	        android:layout_alignParentTop="true"
	        android:gravity="center_vertical"
	        android:text="@string/Bucket_Dump"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_bucketout_on"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="296dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:gravity="left|center_vertical"
		    android:text="@string/On"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

		<RadioButton
		    android:id="@+id/radioButton_menu_body_mode_softstop_bucketout_off"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="503dp"
		    android:button="@drawable/_selector_radio_btn"
		    android:text="@string/Off"
		    android:textSize="22sp"
		    android:textColor="@drawable/_selector_radio_txt_white_amber" />

	</RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_mode_softstop_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >
        
        <ImageView
            android:id="@+id/imageView_menu_body_mode_softstop_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_mode_softstop_low_line_1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_low_bg"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	        android:src="@drawable/main_key_low_line" />
	
	   <ImageView
	       android:id="@+id/imageView_menu_body_mode_softstop_low_line_2"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_low_bg"
	       android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	       android:src="@drawable/main_key_low_line" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_mode_softstop_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	    
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_low_bg"
	        android:layout_toLeftOf="@+id/imageView_menu_body_mode_softstop_low_line_1"
	        android:background="@drawable/_selector_menu_low_center_btn" />
	    
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_mode_softstop_low_default"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_mode_softstop_low_bg"
	        android:layout_toLeftOf="@+id/imageView_menu_body_mode_softstop_low_line_2"
	        android:background="@drawable/_selector_menu_low_left_btn" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_mode_softstop_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	                android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_mode_softstop_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	        android:layout_alignRight="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	        android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_cancel" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_mode_softstop_low_cancel_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	                android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_cancel"
	                android:src="@drawable/menu_low_icon_cancel" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_mode_softstop_low_cancel"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/Cancel"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_default"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_default"
	        android:layout_alignRight="@+id/ImageButton_menu_body_mode_softstop_low_default"
	        android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_default" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_mode_softstop_low_default_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_mode_softstop_low_default"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_mode_softstop_low_default"
	                android:layout_alignTop="@+id/ImageButton_menu_body_mode_softstop_low_default"
	                android:src="@drawable/menu_low_icon_default" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_mode_softstop_low_default"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/Default"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>
</RelativeLayout>
