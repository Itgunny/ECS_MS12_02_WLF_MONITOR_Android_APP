<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="13dp"
            android:src="@drawable/fuel_graph_bg_line" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="13dp"
            android:gravity="center"
            android:text="50" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="69dp"
            android:gravity="center"
            android:text="40" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="129dp"
            android:gravity="center"
            android:text="30" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="189dp"
            android:gravity="center"
            android:text="20" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="249dp"
            android:gravity="center"
            android:text="10" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginTop="309dp"
            android:gravity="center"
            android:text="0" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_unit"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_y_6"
            android:layout_marginLeft="16dp"
            android:gravity="center"
            android:text="@string/l_h" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_1"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginLeft="171dp"
            android:gravity="center"
            android:text="P" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_2"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginLeft="188dp"
            android:layout_toRightOf="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_1"
            android:gravity="center"
            android:text="S" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_3"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginLeft="188dp"
            android:layout_toRightOf="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_2"
            android:gravity="center"
            android:text="E" />

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="171dp"
            android:layout_marginTop="13dp"
            android:src="@drawable/fuel_graph_empty_02" />

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="188dp"
            android:layout_marginTop="13dp"
            android:layout_toRightOf="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_1"
            android:src="@drawable/fuel_graph_empty_02" />

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="188dp"
            android:layout_marginTop="13dp"
            android:layout_toRightOf="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_2"
            android:src="@drawable/fuel_graph_empty_02" />

        <RelativeLayout
            android:id="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph_value1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="173dp" >

            <ImageView
                android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_value1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:src="@drawable/fuel_graph_power_02" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph_value2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="192dp"
            android:layout_toRightOf="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph_value1" >

            <ImageView
                android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_value2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:src="@drawable/fuel_graph_standard_02" />
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph_value3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_bg"
            android:layout_marginBottom="6dp"
            android:layout_marginLeft="192dp"
            android:layout_toRightOf="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_graph_value2" >

            <ImageView
                android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_graph_value3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:scaleType="fitXY"
                android:src="@drawable/fuel_graph_econo_02" />
        </RelativeLayout>

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_text_1"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="171dp"
            android:gravity="center"
            android:text="00" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_text_2"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="188dp"
            android:layout_toRightOf="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_text_1"
            android:gravity="center"
            android:text="00" />

        <TextView
            android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_text_3"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="188dp"
            android:layout_toRightOf="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_graph_text_2"
            android:gravity="center"
            android:text="00" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_monitoring_fuelhistory_moderecord_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />

        <ImageButton
            android:id="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_alignRight="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:background="@drawable/_selector_menu_low_right_btn" />

        <ImageButton
            android:id="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_toLeftOf="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_line_1"
            android:background="@drawable/_selector_menu_low_center_btn" />

        <ImageButton
            android:id="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_initialization"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_toLeftOf="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_line_2"
            android:background="@drawable/_selector_menu_low_left_btn" />

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_line_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
            android:src="@drawable/main_key_low_line" />

        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_line_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_bg"
            android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_cancel"
            android:src="@drawable/main_key_low_line" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
            android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
            android:layout_alignRight="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
            android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true" >

                <ImageView
                    android:id="@+id/imageView_menu_body_monitoring_fuelhistory_moderecord_low_ok_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
                    android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
                    android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_ok"
                    android:src="@drawable/menu_low_icon_ok" />

                <TextView
                    android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_low_ok"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/OK"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_initialization"
            android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_initialization"
            android:layout_alignRight="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_initialization"
            android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_fuelhistory_moderecord_low_initialization" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true" >

                <TextView
                    android:id="@+id/textView_menu_body_monitoring_fuelhistory_moderecord_low_initialization"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/Initialization"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>