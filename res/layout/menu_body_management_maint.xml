<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/Relativelayout_menu_body_management_maint_low"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true" >

        

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp" >

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

            </LinearLayout>
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp" >

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list10"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

            </LinearLayout>
            
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp" >

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list11"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="5dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list13"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />
                
                <ImageButton
                    android:id="@+id/imageButton_menu_body_management_maint_list15"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:background="@drawable/_selector_menu_body_management_maint_list_btn1" />

            </LinearLayout>

        </LinearLayout>

    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/Relativelayout_menu_body_management_maint_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >
       
        <ImageView
            android:id="@+id/imageView_menu_body_management_maint_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_management_maint_low_line"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:src="@drawable/main_key_low_line" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_management_maint_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_management_maint_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_management_maint_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_management_maint_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_management_maint_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_management_maint_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_management_maint_low_ok"
	                android:layout_alignTop="@+id/ImageButton_menu_body_management_maint_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_management_maint_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />

	        </LinearLayout>
	    </RelativeLayout>

	    <TextView
	        android:id="@+id/textView_menu_body_management_maint_low_as"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_management_maint_low_line"
	        android:layout_alignParentLeft="true"
	        android:layout_alignTop="@+id/imageView_menu_body_management_maint_low_line"
	        android:layout_toLeftOf="@+id/imageView_menu_body_management_maint_low_line"
	        android:gravity="center"
	        android:text="@string/AS"
	        android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>
    

</RelativeLayout>
