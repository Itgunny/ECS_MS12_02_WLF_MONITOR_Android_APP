<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >
    
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/RelativeLayout_menu_body_preference_brightness_manual_low"
        android:layout_alignParentTop="true" >

        <TextView
            android:id="@+id/textView_menu_body_preference_brightness_manual_min"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/RelativeLayout_menu_body_preference_brightness_manual_seekbar"
            android:layout_below="@+id/RelativeLayout_menu_body_preference_brightness_manual_seekbar"
            android:layout_marginTop="-10dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/gray"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView_menu_body_preference_brightness_manual_max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignRight="@+id/RelativeLayout_menu_body_preference_brightness_manual_seekbar"
            android:layout_below="@+id/RelativeLayout_menu_body_preference_brightness_manual_seekbar"
            android:layout_marginTop="-10dp"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/gray"
            android:textSize="20sp" />

        <RelativeLayout
            android:id="@+id/RelativeLayout_menu_body_preference_brightness_manual_seekbar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="140dp" >

            <ImageView
                android:id="@+id/imageView_menu_body_preference_brightness_manual_seekbar_bg"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:src="@drawable/menu_preperence_brightness_manual_slide_bg" />

            <SeekBar
                android:id="@+id/seekBar_menu_body_preference_brightness_manual"
                style="@style/RideControlSpeedSeekBar"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/imageView_menu_body_preference_brightness_manual_seekbar_bg"
                android:layout_alignRight="@+id/imageView_menu_body_preference_brightness_manual_seekbar_bg"
                android:maxHeight="7dp"
                android:minHeight="5dp"
                android:paddingLeft="20dp"
                android:paddingRight="20dp"
                android:thumb="@drawable/_selector_seekbar_thumb" />

        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_preference_brightness_manual_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >

        <ImageView
            android:id="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />

        <ImageView
            android:id="@+id/imageView_menu_body_preference_brightness_manual_low_line_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
            android:src="@drawable/main_key_low_line" />

        <ImageView
            android:id="@+id/imageView_menu_body_preference_brightness_manual_low_line_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
            android:src="@drawable/main_key_low_line" />

        <ImageButton
            android:id="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:layout_alignRight="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:background="@drawable/_selector_menu_low_right_btn" />

        <ImageButton
            android:id="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView_menu_body_preference_brightness_manual_low_bg"
            android:layout_toLeftOf="@+id/imageView_menu_body_preference_brightness_manual_low_line_1"
            android:background="@drawable/_selector_menu_low_center_btn" />

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
            android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
            android:layout_alignRight="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
            android:layout_alignTop="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true" >

                <ImageView
                    android:id="@+id/imageView_menu_body_preference_brightness_manual_low_ok_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
                    android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
                    android:layout_alignTop="@+id/ImageButton_menu_body_preference_brightness_manual_low_ok"
                    android:src="@drawable/menu_low_icon_ok" />

                <TextView
                    android:id="@+id/textView_menu_body_preference_brightness_manual_low_ok"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/OK"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
            android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
            android:layout_alignRight="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
            android:layout_alignTop="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel" >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true" >

                <ImageView
                    android:id="@+id/imageView_menu_body_preference_brightness_manual_low_cancel_icon"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
                    android:layout_alignLeft="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
                    android:layout_alignTop="@+id/ImageButton_menu_body_preference_brightness_manual_low_cancel"
                    android:src="@drawable/menu_low_icon_cancel" />

                <TextView
                    android:id="@+id/textView_menu_body_preference_brightness_manual_low_cancel"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="10dp"
                    android:gravity="center"
                    android:text="@string/Cancel"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:textSize="25sp"
                    android:textStyle="bold" />
            </LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
