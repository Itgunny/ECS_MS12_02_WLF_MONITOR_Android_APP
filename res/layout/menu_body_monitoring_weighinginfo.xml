    <RelativeLayout
        android:id="@+id/Relativelayout_menu_body_monitoring_weighinginfo_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" >
       
        <ImageView
            android:id="@+id/imageView_menu_body_monitoring_weighinginfo_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_monitoring_weighinginfo_low_line"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:src="@drawable/main_key_low_line" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_monitoring_weighinginfo_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_monitoring_weighinginfo_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_monitoring_weighinginfo_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_monitoring_weighinginfo_low_ok"
