<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_list_1"
        android:layout_width="match_parent"
        android:layout_height="65dp" >

        <ImageButton
            android:id="@+id/imageButton_menu_body_list_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:background="@drawable/_selector_menu_body_list_btn" />

        <TextView
            android:id="@+id/textView_menu_body_list_1_title"
            android:layout_width="420dp"
            android:layout_height="wrap_content"
            android:layout_alignLeft="@+id/imageView_menu_body_list_line_1"
            android:layout_alignParentBottom="true"
            android:layout_alignParentTop="true"
            android:gravity="center_vertical"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="25sp" />

        <TextView
            android:id="@+id/textView_menu_body_list_1_data"
            android:layout_width="170dp"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
            android:layout_marginRight="20dp"
            android:gravity="center_vertical"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/menu_body_mode_tab_amber" />

        <ImageView
            android:id="@+id/imageView_menu_body_list_line_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_marginLeft="20dp"
            android:src="@drawable/menu_list_line" />

    </RelativeLayout>
    
