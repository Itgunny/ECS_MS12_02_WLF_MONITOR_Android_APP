<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout_key_main_b_workload"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_key_bg" >
    
    <ImageView
        android:id="@+id/imageView_key_main_b_workload_line_1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="105dp"
        android:layout_marginTop="112dp"
        android:src="@drawable/main_key_line" />
    
    <ImageView
        android:id="@+id/imageView_key_main_b_workload_line_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_1"
        android:layout_below="@+id/imageView_key_main_b_workload_line_1"
        android:layout_marginTop="56dp"
        android:src="@drawable/main_key_line" />
    
    <ImageView
        android:id="@+id/imageView_key_main_b_workload_line_3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_1"
        android:layout_below="@+id/imageView_key_main_b_workload_line_2"
        android:layout_marginTop="56dp"
        android:src="@drawable/main_key_line" />

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_workload_accumulation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView_key_main_b_workload_line_1"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_1"
        android:layout_alignRight="@+id/imageView_key_main_b_workload_line_1"
        android:layout_marginBottom="5dp" >
        
        <ImageButton
            android:id="@+id/imageButton_key_main_b_workload_accumulation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/_selector_key_list_btn1" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_accumulation_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_key_main_b_workload_accumulation_title"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/Weighing_System_Mode"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_key_main_b_workload_accumulation_data"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_accumulation_title"
            android:text="@string/Manual"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_accumulation_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_workload_accumulation_data"
            android:layout_alignTop="@+id/textView_key_main_b_workload_accumulation_data"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_accumulation_data"
            android:src="@drawable/main_key_list_arrow" />

        

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_workload_display"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView_key_main_b_workload_line_2"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_2"
        android:layout_alignRight="@+id/imageView_key_main_b_workload_line_2"
        android:layout_marginBottom="5dp" >
        
        <ImageButton
            android:id="@+id/imageButton_key_main_b_workload_display"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/_selector_key_list_btn1" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_display_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_key_main_b_workload_display_title"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/Weighing_Display"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_key_main_b_workload_display_data"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_display_title"
            android:text="@string/Daily"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_display_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_workload_display_data"
            android:layout_alignTop="@+id/textView_key_main_b_workload_display_data"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_display_data"
            android:src="@drawable/main_key_list_arrow" />
        
        
        

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_workload_errordetect"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView_key_main_b_workload_line_3"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_3"
        android:layout_alignRight="@+id/imageView_key_main_b_workload_line_3"
        android:layout_marginBottom="5dp" >
        
        <ImageButton
            android:id="@+id/imageButton_key_main_b_workload_errordetect"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/_selector_key_list_btn1" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_errordetect_point"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_key_main_b_workload_errordetect_title"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:text="@string/Error_Detection"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_key_main_b_workload_errordetect_data"
            android:layout_width="130dp"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_errordetect_title"
            android:text="@string/Off"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_errordetect_arrow"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_key_main_b_workload_errordetect_data"
            android:layout_alignTop="@+id/textView_key_main_b_workload_errordetect_data"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_key_main_b_workload_errordetect_data"
            android:src="@drawable/main_key_list_arrow" />
        
        

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_workload_calibration"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/imageView_key_main_b_workload_line_3"
        android:layout_alignRight="@+id/imageView_key_main_b_workload_line_3"
        android:layout_below="@+id/imageView_key_main_b_workload_line_3"
        android:layout_marginTop="5dp" >
        
        <TextView
            android:id="@+id/textView_key_main_b_workload_calibration"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:background="@drawable/_selector_key_quickcoupler_btn1"
            android:gravity="center"
            android:text="@string/Boom_Pressure_Calibration"
            android:textAppearance="?android:attr/textAppearanceLarge" />

    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/RelativeLayout_key_main_b_workload_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="52dp"
        android:layout_marginTop="303dp" >
        
        <ImageView
            android:id="@+id/imageView_key_main_b_workload_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/main_key_low_bg" />
	    
	    <ImageView
	        android:id="@+id/imageView_key_main_b_workload_low_line_1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_workload_low_bg"
	        android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_ok"
	        android:src="@drawable/main_key_low_line" />
	
	   <ImageView
	       android:id="@+id/imageView_key_main_b_workload_low_line_2"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_alignBottom="@+id/imageView_key_main_b_workload_low_bg"
	       android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_cancel"
	       android:src="@drawable/main_key_low_line" />
	   
	    <ImageButton
	        android:id="@+id/ImageButton_key_main_b_workload_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_workload_low_bg"
	        android:layout_alignRight="@+id/imageView_key_main_b_workload_low_bg"
	        android:background="@drawable/_selector_key_centerright_btn1" />
	    
	    <ImageButton
	        android:id="@+id/ImageButton_key_main_b_workload_low_cancel"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_workload_low_bg"
	        android:layout_toLeftOf="@+id/imageView_key_main_b_workload_low_line_1"
	        android:background="@drawable/_selector_key_centerright_btn1"
	        android:visibility="invisible" />
	    
	    <ImageButton
	        android:id="@+id/ImageButton_key_main_b_workload_low_default"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_key_main_b_workload_low_bg"
	        android:layout_toLeftOf="@+id/imageView_key_main_b_workload_low_line_2"
	        android:background="@drawable/_selector_key_left_btn1" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_ok"
	        android:layout_alignRight="@+id/ImageButton_key_main_b_workload_low_ok"
	        android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_ok" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_key_main_b_workload_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_ok"
	                android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_key_main_b_workload_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_cancel"
	        android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_cancel"
	        android:layout_alignRight="@+id/ImageButton_key_main_b_workload_low_cancel"
	        android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_cancel"
	        android:visibility="invisible" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_key_main_b_workload_low_cancel_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_cancel"
	                android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_cancel"
	                android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_cancel"
	                android:src="@drawable/menu_low_icon_cancel" />
	
	            <TextView
	                android:id="@+id/textView_key_main_b_workload_low_cancel"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/Cancel"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_default"
	        android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_default"
	        android:layout_alignRight="@+id/ImageButton_key_main_b_workload_low_default"
	        android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_default" >
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" >
	
	            <ImageView
	                android:id="@+id/imageView_key_main_b_workload_low_default_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_key_main_b_workload_low_default"
	                android:layout_alignLeft="@+id/ImageButton_key_main_b_workload_low_default"
	                android:layout_alignTop="@+id/ImageButton_key_main_b_workload_low_default"
	                android:src="@drawable/menu_low_icon_default" />
	
	            <TextView
	                android:id="@+id/textView_key_main_b_workload_low_default"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/Default"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>
   
    

</RelativeLayout>
