<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/AbsoluteLayout_screen_main_a"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_a_bg" >

    <FrameLayout
        android:id="@+id/FrameLayout_screen_main_a_menubar"
        android:layout_width="wrap_content"
        android:layout_height="485dp"
        android:layout_x="0dp"
        android:layout_y="-5dp" >
    </FrameLayout>

    <AbsoluteLayout
        android:id="@+id/AbsoluteLayout_screen_main_a_key"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="invisible" >

        <ImageView
            android:id="@+id/imageView_screen_main_a_key_body_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="70dp"
            android:layout_y="65dp"
            android:src="@drawable/main_a_bg_right" />

        <ImageView
            android:id="@+id/imageView_screen_main_a_key_title_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="-97dp"
            android:layout_y="73dp"
            android:src="@drawable/main_a_key_bg" />

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_key_body"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="70dp"
            android:layout_y="65dp" >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_key_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="-97dp"
            android:layout_y="73dp" >
        </FrameLayout>
    </AbsoluteLayout>

    <AbsoluteLayout
        android:id="@+id/AbsoluteLayout_screen_main_a_body"
        android:layout_width="1500dp"
        android:layout_height="wrap_content"
        android:layout_x="-350dp" >


        <ImageView
            android:id="@+id/imageView_screen_main_a_leftdown_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_y="246dp"
            android:background="@drawable/main_a_bg_left_down" />

        <ImageView
            android:id="@+id/imageView_screen_main_a_leftup_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_y="65dp"
            android:src="@drawable/main_a_bg_left" />

        <ImageView
            android:id="@+id/imageView_screen_main_a_rightdown_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="750dp"
            android:layout_y="212dp"
            android:background="@drawable/main_a_bg_right_down" />

        <ImageView
            android:id="@+id/imageView_screen_main_a_rightup_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="750dp"
            android:layout_y="65dp"
            android:background="@drawable/main_a_bg_right" />
        
        <ImageView
            android:id="@+id/imageView_screen_main_a_center_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="583dp"
            android:layout_y="73dp" />

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_leftup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_y="65dp" >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_leftdown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_y="246dp" >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_rightup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="750dp"
            android:layout_y="65dp" >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_rightdown"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="750dp"
            android:layout_y="212dp" >
        </FrameLayout>

        <FrameLayout
            android:id="@+id/FrameLayout_screen_main_a_center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="583dp"
            android:layout_y="73dp" >
        </FrameLayout>
    </AbsoluteLayout>

    <FrameLayout
        android:id="@+id/FrameLayout_screen_main_a_indicator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="0dp" >
    </FrameLayout>

    <FrameLayout
        android:id="@+id/FrameLayout_screen_main_a_virtualkey"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="0dp"
        android:layout_y="65dp"
        android:background="@drawable/main_a_virtual_key_bg_up"
        android:visibility="invisible" >
    </FrameLayout>

</AbsoluteLayout>