<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent" 
    android:splitMotionEvents="false">
    
    
    <ImageView
        android:id="@+id/imageView_menu_body_management_smartkey_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="69dp"
        android:src="@drawable/line_menu_step1" />

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/imageView_menu_body_management_smartkey_line"
        android:layout_alignLeft="@+id/imageView_menu_body_management_smartkey_line"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/imageView_menu_body_management_smartkey_line" 
        android:splitMotionEvents="false">

        <ImageView
            android:id="@+id/imageView_menu_body_management_smartkey_use_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_use_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/imageView_menu_body_management_smartkey_use_title"
            android:text="@string/Manage_Usage"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <RadioGroup
            android:id="@+id/radioGroup_menu_body_management_smartkey_use"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="260dp"
            android:orientation="horizontal" 
            android:splitMotionEvents="false">
	
	       <RadioButton
	           android:id="@+id/radioButton_menu_body_management_smartkey_use_diable"
	           android:layout_width="220dp"
	           android:layout_height="wrap_content"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/Disable"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp" />
	
	       <RadioButton
	           android:id="@+id/radioButton_menu_body_management_smartkey_use_enable"
	           android:layout_width="220dp"
	           android:layout_height="wrap_content"
	           android:layout_marginLeft="20dp"
	           android:button="@drawable/_selector_radio_btn"
	           android:text="@string/Enable"
	           android:textColor="@drawable/_selector_radio_txt_white_amber"
	           android:textSize="25sp" />
	       
   		</RadioGroup>
   
    </RelativeLayout>
    
    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_management_smartkey_tag_contents"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/RelativeLayout_menu_body_management_smartkey_low"
        android:layout_alignLeft="@+id/imageView_menu_body_management_smartkey_line"
        android:layout_alignRight="@+id/imageView_menu_body_management_smartkey_line"
        android:layout_below="@+id/imageView_menu_body_management_smartkey_line" 
        android:splitMotionEvents="false">
        
        <ImageView
            android:id="@+id/imageView_menu_body_management_smartkey_tag_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:src="@drawable/menu_bluedot" />

        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_toRightOf="@+id/imageView_menu_body_management_smartkey_tag_title"
            android:text="@string/Manage_Tag"
            android:textAppearance="?android:attr/textAppearanceLarge" />
        
        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_register_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView_menu_body_management_smartkey_tag_title"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="20dp"
            android:text="@string/Registered_Tag"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_register_colon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_menu_body_management_smartkey_tag_register_title"
            android:layout_alignTop="@+id/textView_menu_body_management_smartkey_tag_register_title"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_menu_body_management_smartkey_tag_register_title"
            android:text=":"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_register_num"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView_menu_body_management_smartkey_tag_register_colon"
            android:layout_alignTop="@+id/textView_menu_body_management_smartkey_tag_register_colon"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView_menu_body_management_smartkey_tag_register_colon"
            android:textAppearance="?android:attr/textAppearanceLarge" />

        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_detail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/textView_menu_body_management_smartkey_tag_warning"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_below="@+id/textView_menu_body_management_smartkey_tag_register_title"
            android:gravity="center"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textSize="25sp" />
        
        <TextView
            android:id="@+id/textView_menu_body_management_smartkey_tag_warning"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/LinearLayout_menu_body_management_smartkey_tag_btn"
            android:layout_alignParentLeft="true"
            android:layout_alignParentRight="true"
            android:layout_marginBottom="10dp"
            android:gravity="center"
            android:text="@string/Recommend_deleting_unused_tag_for_security_when_you_register_new_tag"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:textColor="@color/main_title_amber" />

        <LinearLayout
            android:id="@+id/LinearLayout_menu_body_management_smartkey_tag_btn"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="10dp" 
            android:splitMotionEvents="false">

	       <TextView
	            android:id="@+id/textView_menu_body_management_smartkey_tag_registration"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="20dp"
	            android:background="@drawable/_selector_menu_body_management_smartkey_btn1"
	            android:gravity="center"
	            android:text="@string/Registration"
	            android:textAppearance="?android:attr/textAppearanceLarge" />
	        
	        <TextView
	            android:id="@+id/textView_menu_body_management_smartkey_tag_delete"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_marginLeft="20dp"
	            android:background="@drawable/_selector_menu_body_management_smartkey_btn1"
	            android:gravity="center"
	            android:text="@string/Delete"
	            android:textAppearance="?android:attr/textAppearanceLarge" />
        </LinearLayout>

    </RelativeLayout>
    

    <RelativeLayout
        android:id="@+id/RelativeLayout_menu_body_management_smartkey_low"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" 
        android:splitMotionEvents="false">
        
        <ImageView
            android:id="@+id/imageView_menu_body_management_smartkey_low_bg"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/menu_low_bg" />
	    
	    
	   
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_management_smartkey_low_bg"
	        android:layout_alignRight="@+id/imageView_menu_body_management_smartkey_low_bg"
	        android:background="@drawable/_selector_menu_low_right_btn" />
	    
	    <ImageButton
	        android:id="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_management_smartkey_low_bg"
	        android:layout_toLeftOf="@+id/imageView_menu_body_management_smartkey_low_line_1"
	        android:background="@drawable/_selector_menu_low_center_btn" />
	    
	    <ImageView
	        android:id="@+id/imageView_menu_body_management_smartkey_low_line_1"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/imageView_menu_body_management_smartkey_low_bg"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	        android:src="@drawable/main_key_low_line" />
	
	   <ImageView
	       android:id="@+id/imageView_menu_body_management_smartkey_low_line_2"
	       android:layout_width="wrap_content"
	       android:layout_height="wrap_content"
	       android:layout_alignBottom="@+id/imageView_menu_body_management_smartkey_low_bg"
	       android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	       android:src="@drawable/main_key_low_line" />
	
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	        android:layout_alignRight="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	        android:layout_alignTop="@+id/ImageButton_menu_body_management_smartkey_low_ok" 
	        android:splitMotionEvents="false">
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" 
	            android:splitMotionEvents="false">
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_management_smartkey_low_ok_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	                android:layout_alignTop="@+id/ImageButton_menu_body_management_smartkey_low_ok"
	                android:src="@drawable/menu_low_icon_ok" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_management_smartkey_low_ok"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/OK"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>
	    
	    <RelativeLayout
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignBottom="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	        android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	        android:layout_alignRight="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	        android:layout_alignTop="@+id/ImageButton_menu_body_management_smartkey_low_cancel" 
	        android:splitMotionEvents="false">
	
	        <LinearLayout
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content"
	            android:layout_centerHorizontal="true"
	            android:layout_centerVertical="true" 
	            android:splitMotionEvents="false">
	
	            <ImageView
	                android:id="@+id/imageView_menu_body_management_smartkey_low_cancel_icon"
	                android:layout_width="wrap_content"
	                android:layout_height="wrap_content"
	                android:layout_alignBottom="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	                android:layout_alignLeft="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	                android:layout_alignTop="@+id/ImageButton_menu_body_management_smartkey_low_cancel"
	                android:src="@drawable/menu_low_icon_cancel" />
	
	            <TextView
	                android:id="@+id/textView_menu_body_management_smartkey_low_cancel"
	                android:layout_width="wrap_content"
	                android:layout_height="match_parent"
	                android:layout_marginLeft="10dp"
	                android:gravity="center"
	                android:text="@string/Cancel"
	                android:textAppearance="?android:attr/textAppearanceLarge"
	                android:textSize="25sp"
	                android:textStyle="bold" />
	
	        </LinearLayout>
	    </RelativeLayout>

    </RelativeLayout>

</RelativeLayout>
